{
  "name": "Jelly-Party",
  "version": "1.2.4",
  "description": "An extension that synchronizes video playback!",
  "permissions": ["activeTab", "storage"],
  "optional_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo/16x16.png",
      "32": "images/logo/32x32.png",
      "48": "images/logo/48x48.png",
      "128": "images/logo/128x128.png"
    }
  },
  "icons": {
    "16": "images/logo/16x16.png",
    "32": "images/logo/32x32.png",
    "48": "images/logo/48x48.png",
    "128": "images/logo/128x128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; connect-src 'self'; object-src 'self'",
  "web_accessible_resources": ["js/IFrameStyles.js", "js/RootStyles.js"]
}
